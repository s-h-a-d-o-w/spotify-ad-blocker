{
  "name": "spotify-ad-blocker",
  "version": "1.0.2",
  "engines": {
    "node": ">=10.5.0"
  },
  "bin": "./src/main.js",
  "main": "./src/main.js",
  "license": "MIT",
  "scripts": {
    "beforebuild": "node-gyp rebuild && renamer -f .node -r .foolpkg **/*.node",
    "build": "npm run beforebuild && npm run build:pkg && npm run build:silentexe && npm run afterbuild",
    "build:nonsilent": "npm run beforebuild && npm run build:pkg && npm run afterbuild",
    "build:pkg": "pkg . --target win --output spotify-ad-blocker.exe",
    "build:silentexe": "create-nodew-exe spotify-ad-blocker.exe spotify-ad-blocker.exe",
    "afterbuild": "renamer -f .foolpkg -r .node **/*.foolpkg",
    "start": "cross-env DEBUG=swh node .",
    "start:webstorm": "npm run build:mute && cross-env DEBUG=* node %NODE_DEBUG_OPTION% .",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "auto-launch": "5.0.5",
    "bindings": "1.3.0",
    "fecha": "2.3.3",
    "fs-extra": "6.0.1",
    "node-native-dialog": "^1.0.1",
    "process-list": "1.2.3",
    "renamer": "^0.7.3",
    "spotify-web-helper": "s-h-a-d-o-w/spotify-web-helper#dev",
    "systray": "1.0.5"
  },
  "devDependencies": {
    "create-nodew-exe": "1.0.4",
    "cross-env": "5.1.4",
    "patch-package": "5.1.1",
    "pkg": "4.3.3",
    "postinstall-postinstall": "1.0.0"
  },
  "pkg": {
    "assets": [
      "./assets/spotify-ad-blocker.ico",
      "./node_modules/process-list/build/Release/processlist.foolpkg",
      "./build/Release/volumectrl.foolpkg",
      "./node_modules/winax/build/Release/node_activex.foolpkg",
      "./node_modules/systray/traybin/tray_windows_release.exe"
    ]
  }
}
